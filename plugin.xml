<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.codemech.yandexads" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
<name>cordova-plugin-yandexads</name>
<js-module name="cordova-plugin-yandexads" src="www/YandexAds.js">
	<clobbers target="cordova.plugins.cordova-plugin-yandexads" />
</js-module>
<platform name="android">
	<config-file parent="/*" target="res/xml/config.xml">
		<feature name="YandexAds">
			<param name="android-package" value="com.codemech.yandexads.YandexAds" />
		</feature>
	</config-file>
	<config-file parent="/*" target="AndroidManifest.xml"></config-file>
	<source-file src="src/android/YandexAds.java" target-dir="src/com/codemech/yandexads" />
</platform>
</plugin>